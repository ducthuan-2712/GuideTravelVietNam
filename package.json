    "husky": "^0.14.3",
    "jest": "17.0.3",
    "prettier": "^1.7.0",
    "react-test-renderer": "15.4.1",
    "relay-compiler": "^1.4.0",
    "xcode": "^0.9.3",
    "eslint": ">=4.18.2"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
